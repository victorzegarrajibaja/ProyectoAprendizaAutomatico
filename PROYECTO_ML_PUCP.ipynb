        "id": "m-i9aopqOS1W",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "cols = ['job', 'contact', 'marital','education', 'poutcome', 'month', 'day_of_week']\n",
        "for label in cols:\n",
        "    label_encoder = LabelEncoder()\n",
        "    label_encoder.fit(data[label])\n",
        "    data[label] = label_encoder.transform(data[label])"
      ],
      "execution_count": null,
      "outputs": []
